<?php


add_action( 'init', '<%= theme_id %>_init' );

if ( ! function_exists( '<%= theme_id %>_init' ) ) :

/**
* Theme init actions
*/
function <%= theme_id %>_init() {

	if ( ! is_admin() ) :
		wp_enqueue_style(
			'<%= theme_id %>-style',
			get_bloginfo( 'stylesheet_url' )
		);
	endif;

}

endif; // <%= theme_id %>_init

add_action( 'after_setup_theme', '<%= theme_id %>_setup' );

if ( ! function_exists( '<%= theme_id %>_setup' ) ) :

/**
* Set up your theme here
*/
function <%= theme_id %>_setup() {
	add_theme_support( 'post-thumbnails' );
}

endif; // <%= theme_id %>_setup
