// This is your main stylesheet file, you can use SASS in here to include css partials

@import "reset";
@import "typography";

@import "blueprint/grid";

.clear {
  clear: both;
}

#container {
  @include container;
  @include prepend(1);
  border-left: 1px solid;
  border-right: 1px solid;
}

#content {
  @include column(15);
  @include border(#555, 2px);
}

#sidebar {
  @include column(7);
  @include prepend(1);
  @include last;
}